/* pages/songlist_more/songlist_more.wxss */
.searchbox{
width: 85%;
height: 100rpx;
margin: 10rpx auto;
border-radius: 80rpx;
background-color:  #f2f2f2;
}
.searchbox .txt{
  width: 60%;
  height: 90rpx;
  padding-left: 10%;
  float: left;
}
.searchbox .btn{
  width: 20%;
  height: 100rpx;
  float: left;
  text-align: center;
}
.btn image{
  margin-top: 20rpx;
  margin-left: 70rpx;
  width: 50rpx;
  height: 50rpx;
}
.list_warp{
  width: 100%;
  height: 180rpx;
  padding: 10rpx 0rpx;
  box-shadow: 3rpx 6rpx 3rpx #f1f1f1;
  background-color:  #ebfaeb;
  margin-bottom: 2rpx;
}
.list_warp view{
  float: left;
}
.list_warp .list_l{
  width: 130rpx;
  height: 160rpx;
  padding-left: 55rpx;
  padding-top: 20rpx;
}
.list_warp .list_c{
  width: 330rpx;
  height: 160rpx;
  padding: 20rpx;
}
.list_c view{
  width: 250rpx;
  padding-left: 60rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.list_c .musicName{
  font-size: 30rpx;
  height: 80rpx;
  line-height: 80rpx;
}
.list_c .name{
  font-size: 26rpx;
  color: #555;
}
.list_warp .list_r{
  width: 160rpx;
  height: 145rpx;
  padding-top:55rpx;
}
.list_l image{
  width: 160rpx;
  height: 160rpx;
  border-radius: 23rpx;
}
.list_r image{
width: 165rpx;
height: 75rpx;
}
